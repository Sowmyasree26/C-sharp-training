@model IEnumerable<Question1.Models.Customer>

@{
    ViewBag.Title = "Customers from Germany";
}

<h2>Customers from Germany</h2>

<table border="1">
    <tr>
        <th>Customer ID</th>
        <th>Company Name</th>
        <th>Contact Name</th>
        <th>Country</th>
    </tr>

    @foreach (var customer in Model)
    {
        <tr>
            <td>@customer.CustomerID</td>
            <td>@customer.CompanyName</td>
            <td>@customer.ContactName</td>
            <td>@customer.Country</td>
        </tr>
    }
</table>
